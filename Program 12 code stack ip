#include <stdio.h>
#include <ctype.h>
char stack[50];
int top = -1;
void push(char c) { stack[++top] = c; }
char pop() { return stack[top--]; }
int priority(char c) {
    if (c == '+' || c == '-') return 1;
    if (c == '*' || c == '/') return 2;
    return 0;
}
int main() {
    char infix[50], postfix[50];
    int i = 0, k = 0;
    scanf("%s", infix);
    while (infix[i]) {
        if (isalnum(infix[i]))
            postfix[k++] = infix[i];
        else {
            while (top != -1 && priority(stack[top]) >= priority(infix[i]))
                postfix[k++] = pop();
            push(infix[i]);
        }
        i++;
    }
    while (top != -1)
        postfix[k++] = pop();
    postfix[k] = '\0';
    printf("%s\n", postfix);
    return 0;
}
